<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="Day8" extends="json-default">
        <action name="showDepart" class="com.lanou.action.BAction" method="showDept">
            <result name="success" type="json">
                <param name="root">departmentList</param>
            </result>
        </action>
        <action name="showStaff" class="com.lanou.action.BAction" method="showStaff">
            <result name="success" type="json">
                <param name="root">staffList</param>
            </result>
        </action>
    </package>
</struts>